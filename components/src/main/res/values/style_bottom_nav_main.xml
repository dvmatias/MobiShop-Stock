<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomNavIcon">
        <item name="android:tint">@color/colorMain</item>
    </style>

    <style name="BottomNavLabel">
        <item name="android:lines">2</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:translationY">-3.5dp</item>
        <item name="android:textSize">10sp</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:lineSpacingExtra">-1dp</item>
        <item name="android:fontFamily">@font/open_sans_bold</item>
        <item name="android:gravity">center_horizontal|top</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/colorMain</item>
    </style>

</resources>